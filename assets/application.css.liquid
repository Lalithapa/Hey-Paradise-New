/* glass card */
.glass-card {
  position: relative;
  width: 100%;
  height: 84px;

  display: flex;
  justify-content: center;
  align-items: flex-start;
  column-gap: 4px;

  border-radius: 3px;
  padding: 4px;

  background: rgba(255, 255, 255, 0.8);
}

.glass-card-featured-media {
  /* width: 20%; */
  height: 100%;
  object-fit: contain;
  aspect-ratio: 3/4 !important;
  border-radius: 2px;
}

.glass-card-content {
  flex-grow: 1;
  /* height: auto; */
  height: 100%;

  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
  row-gap: 6px;
}

.glass-card-vendor,
.glass-card-title,
.glass-card-price,
.glass-card-cpmare-at-price {
  font-style: normal;
  font-weight: 400;
  margin: 0;
  padding: 0;
}

.glass-card-vendor {
  color: #7e7e7e;
  font-size: 8px;
  line-height: 14px;
  /* height: 14px; */
  text-transform: capitalize;
}

.glass-card-title {
  color: #191919;
  font-size: 11px;
  line-height: 12px;
  /* height: 12px; */
  text-transform: uppercase;

  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.glass-card-title-wrapper{
  width: 100%;
  height: 44px;
}

.glass-card-price-wrapper {
  /* margin-top: 4px; */
  width: 100%;
  flex-grow: 1;

  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: flex-end;

  column-gap: 4px;
}

.glass-card-price,
.glass-card-compare-at-price {
  display: inline;
  color: #373737;
  font-size: 11px;

  line-height: 12px; 
  /* height: 12px; */
  text-transform: capitalize;
}

.glass-card-compare-at-price {
  color: #C0C0C0;
}

.glass-card-fab {
  height: 24px;
  aspect-ratio: 1;
  margin-top: auto;
  background-color: #f6f7ff;
  border-radius: 50%;

  /* for text */
