.product-preview-track-prev, 
.product-preview-track-next {
  background: none;
  color: inherit;
  border: none;
  padding: 0;
  font: inherit;
  cursor: pointer;
  outline: inherit;

  height: 48px;
  aspect-ratio: 1;
  display: none;
  justify-content: center;
  align-items: center;
}

.product-preview-track-prev[disabled], 
.product-preview-track-next[disabled] {
  opacity: 0.5;
}


@media only screen and (min-width: 1024px) {
  .product-preview-track-prev,
  .product-preview-track-next {
