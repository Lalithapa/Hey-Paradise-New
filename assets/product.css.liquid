.varient-container .m-live-views {
  position: absolute;
  right: 10px;
  top: 10px;
  display: flex;
  align-items: center;
  justify-content: end;
  z-index: 1
}

.varient-container .varient-fav-container {
  position: absolute;
  right: 20px;
  top: 10px;
  display: inline-flex;
  align-items: center;
  justify-content: end;
  z-index: 1;
  width: max-content;
  padding: 0;
}


.varient-container .varient-fav-container .active-fav::after,
.varient-container .varient-fav-container .active-fav::before {
  content: url("../images/Heart states/after.svg");
  position: absolute;
  width: 100%;
  height: 100%;
  right: 0;
  transform: scale(1.1);
  top: 5px
}

