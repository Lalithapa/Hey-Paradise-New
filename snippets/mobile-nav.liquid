<div class='mobile-nav d-lg-none'>
  <div class='m-header'>
    <img
      loading='lazy'
      src='https://cdn.shopify.com/s/files/1/0673/6396/3172/files/Logo.svg?v=1694073877'
      alt='logo'
      class='img-fluid'
      width='90px'
    >
    <button
      class='slide-close d-flex align-items-center justify-content-center border-0 bg-transparent menu-slide-toggle'
    >
      <svg
        xmlns='http://www.w3.org/2000/svg'
        width='13'
        height='14'
        viewBox='0 0 13 14'
        fill='none'
      >
        <path d="M0.5 1L12.5 13" stroke="#393939" stroke-linecap="round" />
        <path d="M12.5 1L0.5 13" stroke="#393939" stroke-linecap="round" />
      </svg>
    </button>
  </div>
  <div class='m-content'>
    <div class='m-search'>
      <input
        type='text'
        class='m-search-input'
        placeholder='What are you looking for?'
      >
      <span class='m-search-icon'>
        <svg
          xmlns='http://www.w3.org/2000/svg'
          width='23'
          height='22'
          viewBox='0 0 23 22'
          fill='none'
        >
          <circle cx="10.25" cy="9.75" r="8.75" stroke="#393939" stroke-width="1.5" />
          <path d="M21.5 21L17.75 17.25" stroke="#393939" stroke-width="1.5" stroke-linecap="round" />
        </svg>
      </span>
    </div>

    <div class="m-banner" id="m-nav-banner" 
      {% if customer %} data-slick='{"dots": false,"infinite": true, "slidesToShow": 1,"autoplay":true, "slidesToScroll": 1, "arrows": false}'{% endif %} >
      {% if customer %}
      <div class="position-relative">
        <img loading="lazy" width="100%" height="auto"
          src="https://cdn.shopify.com/s/files/1/0577/9000/5290/files/Rectangle_4127_1.png?v=1709292358"
          alt="hey-banner" class="banner-img">
        <div class="m-banner-info-container">
          <p class="m-banner-text">Hey! {{ customer.first_name }}</p>
          <p class="m-banner-sub-text">Get many surprises</p>
          <a class="m-banner-sub-text" href="/account">View Profile <span>
              <svg xmlns="http://www.w3.org/2000/svg" width="8" height="9" viewBox="0 0 8 9" fill="none">
                <path d="M1.5 1.32202L5.5 4.82202L1.5 8.32202" stroke="#393939" stroke-width="1.2"
                  stroke-linecap="round" stroke-linejoin="round" />
              </svg>
            </span></a>
        </div>
      </div>
      <div class="position-relative ">
        <a href="/">
          <img loading="lazy" width="100%" height="auto"
            src="https://cdn.shopify.com/s/files/1/0577/9000/5290/files/Rectangle_4127.png?v=1709291064"
            alt="hey-banner" class="banner-img"></a>
      </div>
      {% else %}
      <div class="position-relative ">
        <img loading="lazy" width="100%" height="auto"
          src="https://cdn.shopify.com/s/files/1/0577/9000/5290/files/Rectangle_4127_1.png?v=1709292358"
          alt="hey-banner" class="banner-img">
        <div class="m-banner-info-container">
          <p class="m-banner-text">Join the Hey! Fam</p>
          <p class="m-banner-sub-text">Get exclusive rewards, offers and more</p>
          <a class="m-banner-sub-text" href="/account">Sign Up/Login <span>
              <svg xmlns="http://www.w3.org/2000/svg" width="8" height="9" viewBox="0 0 8 9" fill="none">
                <path d="M1.5 1.32202L5.5 4.82202L1.5 8.32202" stroke="#393939" stroke-width="1.2"
                  stroke-linecap="round" stroke-linejoin="round" />
              </svg>
            </span></a>
        </div>
      </div>
      {% endif %}
    </div>

    <div class='m-trending'>
      <p class='m-0 m-trending-title'>Trending</p>
      <div class='m-trending-tags-container'>
        <a href='' class='m-trending-tag rounded-pill'>
          <span>
            <svg
              xmlns='http://www.w3.org/2000/svg'
              width='8'
              height='8'
              viewBox='0 0 8 8'
              fill='none'
            >
              <path
                  d="M3.43605 1.18852L6.44465 1.0425M6.44465 1.0425L6.59067 4.05111M6.44465 1.0425L1.0773 6.95742"
                  stroke="#404184" stroke-width="1.2" stroke-linecap="round"
                  stroke-linejoin="round" />
            </svg>
          </span>
          Ohmy Gel Long Lasting Eyeliner
        </a>
        <a href='' class='m-trending-tag rounded-pill' style='color:#D45986'>
          <span>
            <svg
              xmlns='http://www.w3.org/2000/svg'
              width='8'
              height='8'
              viewBox='0 0 8 8'
              fill='none'
            >
              <path
                  d="M3.43605 1.18852L6.44465 1.0425M6.44465 1.0425L6.59067 4.05111M6.44465 1.0425L1.0773 6.95742"
                  stroke="#D45986" stroke-width="1.2" stroke-linecap="round"
                  stroke-linejoin="round" />
            </svg>
          </span>
          Blush Bomb
        </a>
        <a href='' class='m-trending-tag rounded-pill'>
          <span>
            <svg
              xmlns='http://www.w3.org/2000/svg'
              width='8'
              height='8'
              viewBox='0 0 8 8'
              fill='none'
            >
              <path
                  d="M3.43605 1.18852L6.44465 1.0425M6.44465 1.0425L6.59067 4.05111M6.44465 1.0425L1.0773 6.95742"
                  stroke="#404184" stroke-width="1.2" stroke-linecap="round"
                  stroke-linejoin="round" />
            </svg>
          </span>
          Ohmy Gel Long Lasting Eyeliner
        </a>
      </div>
    </div>
    <div class='m-mega-menu megamenus-item'>
      <ul class="m-menu-container d-flex flex-column p-0">
        {% for link in linklists['main-mega-menu'].links -%}
        {% render 'nav-list', link: link, index: 0, showSearchBtn: true %}
        {% endfor %}
      </ul>
    </div>

