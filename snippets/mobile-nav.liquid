{% if customer %}
<script>
  $('#m-nav-banner').slick();
</script>
{% endif %}
<script>
   $(document).ready(function () {
  const openMenu = () => {
    $('#catagory-section').addClass('d-none');
    $('.mobile-nav').toggleClass('mobile-nav-open');
    $('.filter-container').toggleClass('d-none');
    $('html').toggleClass('overflow-hidden');
  };
  $('.menu-slide-toggle').on('click', openMenu);
  });
</script>