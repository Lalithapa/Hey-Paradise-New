     input#ssw-reward-flexible-redeem-input-discount-range-popup {
	--thumb-height: 1.125em;
	--track-height: 4px;
	--track-color: #F2F2F2;
	--brightness-hover: 100%;
	--brightness-down: 80%;
}

input#ssw-reward-flexible-redeem-input-discount-range-popup {
	color: #2b2d42;

	--thumb-height: 12px;
	--thumb-width: 12px;
	--clip-edges: 0.0125em;
}

/* === range commons === */
input#ssw-reward-flexible-redeem-input-discount-range-popup {
	position: relative;
	overflow: hidden;
}

input#ssw-reward-flexible-redeem-input-discount-range-popup:active {
	cursor: grabbing;
}

/* === WebKit specific styles === */
input#ssw-reward-flexible-redeem-input-discount-range-popup,
input#ssw-reward-flexible-redeem-input-discount-range-popup::-webkit-slider-runnable-track,
input#ssw-reward-flexible-redeem-input-discount-range-popup::-webkit-slider-thumb {
	-webkit-appearance: none;
	transition: all ease 100ms;
	height: var(--thumb-height);
}

input#ssw-reward-flexible-redeem-input-discount-range-popup::-webkit-slider-runnable-track,
input#ssw-reward-flexible-redeem-input-discount-range-popup::-webkit-slider-thumb {
	position: relative;
}

input#ssw-reward-flexible-redeem-input-discount-range-popup::-webkit-slider-thumb {
	--thumb-radius: calc((var(--thumb-height) * 0.5) - 1px);
	--clip-top: calc((var(--thumb-height) - var(--track-height)) * 0.5 - 0.5px);
	--clip-bottom: calc(var(--thumb-height) - var(--clip-top));
	--clip-further: calc(100% + 1px);
	--box-fill: calc(-100vmax - var(--thumb-width, var(--thumb-height))) 0 0
		100vmax #C2C4F0;

	width: var(--thumb-width, var(--thumb-height));
	background: linear-gradient(#C2C4F0 0 0) scroll no-repeat left center /
		50% calc(var(--track-height) + 1px);
	background-color: #C2C4F0;
	box-shadow: var(--box-fill);
	border-radius: var(--thumb-width, var(--thumb-height));

	filter: brightness(100%);
	clip-path: polygon(
		100% -1px,
		var(--clip-edges) -1px,
		0 var(--clip-top),
		-100vmax var(--clip-top),
		-100vmax var(--clip-bottom),
		0 var(--clip-bottom),
		var(--clip-edges) 100%,
		var(--clip-further) var(--clip-further)
	);
}

input#ssw-reward-flexible-redeem-input-discount-range-popup:hover::-webkit-slider-thumb {
	filter: brightness(var(--brightness-hover));
	cursor: grab;
}

input#ssw-reward-flexible-redeem-input-discount-range-popup::-webkit-slider-runnable-track {
	background: linear-gradient(var(--track-color) 0 0) scroll no-repeat center /
		100% calc(var(--track-height) + 1px);
}

input#ssw-reward-flexible-redeem-input-discount-range-popup:disabled::-webkit-slider-thumb {
	cursor: not-allowed;
}

