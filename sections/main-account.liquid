  // order-slide js =======================================================================
  const closeSlides = () => {
    //$(".order-overlay").removeClass("order-overlay-open");
    $('.order-details-slide').removeClass('order-details-slide-open');
    $('.add-address-slide').removeClass('add-address-slide-open');
    $('.cart-overlay').removeClass('cart-overlay-open');
  };
  const openOrderHistorySlide = () => {
    //$(".order-overlay").addClass("order-overlay-open");
    // $('.cart-overlay').addClass('cart-overlay-open');
    $('.order-history-container').addClass('order-history-container-open');
    $('.hamburger_menu').addClass('d-none');
    $('.hamburger-back-btn').removeClass('d-none');
  };
  const openAddressSlide = () => {
    //$(".order-overlay").addClass("order-overlay-open");
    //$('.cart-overlay').addClass('cart-overlay-open');
    $('.profile-addresses-container').addClass('profile-addresses-container-open');
    $('.hamburger_menu').addClass('d-none');
    $('.hamburger-back-btn').removeClass('d-none');
  };
  function openOrderDetailsSlide() {
         if ($(window).width() > 992) $('.cart-overlay').addClass('cart-overlay-open');
    const orderId = $(this).attr('data-toggle-slide');
    $('.order-details-slide').removeClass('order-details-slide-open');
    $(`.order-details-slide[data-slide-for=${orderId}]`).addClass('order-details-slide-open');
    window.scrollTo(0,0);
  }
  const openAddAddressSlide = () => {
    $('.cart-overlay').addClass('cart-overlay-open');
    //$(".order-overlay").addClass("order-overlay-open");
    $('.add-address-slide').addClass('add-address-slide-open');
    $('.profile-addresses-container').removeClass('profile-addresses-container-open');
  };

  $('.order-overlay, .od-slide-close-btn, .add-address-slide-close-btn').on('click', closeSlides);
  $('.order-slide-toggle').on('click', openOrderHistorySlide);

  $('.ordered-products, .order-history-heading').on('click', openOrderDetailsSlide);

  $('.address-slide-toggle').on('click', openAddressSlide);
  //$(".add-address-btn").on("click", openAddAddressSlide);

  $('.hamburger-back-btn').on('click', () => {
    if ($('.order-details-slide').hasClass('order-details-slide-open')) {
      $('.order-details-slide').removeClass('order-details-slide-open');
    } else if ($('.order-history-container').hasClass('order-history-container-open')) {
      $('.order-history-container').removeClass('order-history-container-open');
      $('.hamburger_menu').removeClass('d-none');
      $('.hamburger-back-btn').addClass('d-none');
    } else if ($('.profile-addresses-container').hasClass('profile-addresses-container-open')) {
      $('.profile-addresses-container').removeClass('profile-addresses-container-open');
      $('.hamburger_menu').removeClass('d-none');
      $('.hamburger-back-btn').addClass('d-none');
    }
  });

  $('.edit-address-btn').on('click', function () {
    let addressId = $(this).attr('id');
    let editAddress = $(`#EditAddress_${addressId}`);
    $('.editaddresses').hide(50);
    editAddress.show(50);
    $('.edit-address-btn').show(); // edit Pencil btn
    $(this).hide();
  });
  $('.edit-close-btn').on('click', function () {
    // .edit-close-btn close btn
    $('.editaddresses').hide(50); //content
    $('.edit-address-btn').show(); // edit Pencil btn
  });

  //

  $('.addrrss-btn').on('click', function () {
    if (window.location.pathname === '/account') {
      window.location.href = '/account/addresses';
    }
  });

  tabControl();
  var resizeTimer;
  $(window).on('resize', function (e) {
    clearTimeout(resizeTimer);
    resizeTimer = setTimeout(function () {
      tabControl();
    }, 250);
  });

  function tabControl() {
    var tabs = $('.profile-page-container').find('.profile-sidebar-tabs');
    if (tabs.is(':visible')) {
      tabs.find('.sidebar-tabs').on('click', function (event) {
        event.preventDefault();
        var target = $(this).attr('data-target-tab'),
          tabs = $(this).parents('.profile-sidebar-tabs'),
          buttons = tabs.find('.sidebar-tabs'),
          item = tabs.parents('.profile-page-container').find('.profile-mobile-tab-btn');
        buttons.removeClass('active');
        item.removeClass('active');
        $(this).addClass('active');
        $(target).addClass('active');
      });
    } else {
      $('.profile-mobile-tab-btn').on('click', function () {
        var container = $(this).parents('.profile-page-container'),
          currId = $(this).attr('id'),
          items = container.find('.profile-mobile-tab-btn');
        container.find('.profile-sidebar-tabs .sidebar-tabs').removeClass('active');
        items.removeClass('active');
        $(this).addClass('active');
        container.find('.profile-sidebar-tabs .sidebar-tabs[data-target-tab$="#' + currId + '"]').addClass('active');
      });
    }
  }

  const retrieveLocalStorage = () => {
    $('#my-addresses, #orders, #myInfo').click(function () {
      let id = $(this).attr('id');
      localStorage.setItem('lastClickedID', id);
    });

    $('div.sidebar-tabs').click(function () {
      let HashId = $(this).attr('data-target-tab');
      let id = HashId.slice(1);
      localStorage.setItem('lastClickedID', id);
    });

    let lastClickedID = localStorage.getItem('lastClickedID');
    console.log(lastClickedID);
    lastClickedID == null ? (lastClickedID = 'myInfo') : null;
    let activeTab = $('.sidebar-tabs').attr('data-target-tab');
    $('.profile-mobile-tab-btn').removeClass('active');
    $('.profile-mobile-tab-btn#' + lastClickedID).addClass('active');
    $('.sidebar-tabs[data-target-tab="' + lastClickedID + '"]').addClass('active');
    // localStorage.removeItem('lastClickedID');
  };

  window.addEventListener('load', retrieveLocalStorage);
</script>

