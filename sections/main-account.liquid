                        <div class="od-sub-total-container d-flex justify-content-between">
                          <span class="od-sub-total-text">MRP ( {{ totalItems }} Items)</span>
                          <span class="od-sub-total-amount">₹{{ order.subtotal_price | money_without_currency }}</span>
                        </div>
                        <div class="od-shipping-container d-flex justify-content-between">
                          <span class="od-sub-total-text">Bag Discount</span>
                          <span class="od-sub-total-amount">₹{{ order.total_discount | plus: 0 |  money_without_currency }}</span>
                        </div>
