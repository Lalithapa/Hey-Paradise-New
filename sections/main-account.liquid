                          {% else %}
                          <span class="od-shipping-amount">₹{{ order.shipping_price | money_without_currency }}</span>
                          {% endif %}
                        </div>
                        <div class="od-total-container d-flex justify-content-between">
                          <span class="od-total-text">Total</span>
                          <span class="od-total-amount">₹ {{ order.total_price | money_without_currency }}</span>
                        </div>
                      </div>
                    </div>

