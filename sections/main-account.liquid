        <div class="profile-tab-main-content">
          <input
            type="text"
            class="my-info-username"
            placeholder="Enter Name*"
            {% if customer.first_name != blank %}
              value="{{ customer.first_name }}" readonly
            {% else %}
              value="{{ form.first_name }}" required
            {% endif %}
          >
          <input
            type="text"
            class="my-info-email"
            placeholder="Enter Email (Sippin' insider tea)"
            {% if customer.email != blank %}
              value="{{ customer.email }}" readonly
            {% else %}
              value="{{ form.email }}" required
            {% endif %}
          >
          <div class="my-info-mobile-container d-flex">
            {% comment %}
              <select name="" id="" class="my-info-code">
               <option value="+91" selected>+91</option>
                <option value="+11">+11</option>
               <option value="+61">+61</option>
               <option value="+01">+01</option>
               <option value="+92">+92</option>
              </select>
            {% endcomment %}
            <input
              type="tel"
              class="my-info-mobile"
              inputmode="numeric"
              placeholder="Enter Mobile No."
              {% if customer.phone != blank %}
                value="{{ customer.phone }}" readonly
              {% else %}
                value="{{ form.phone }}" inputmode="numeric" pattern="[0-9]{10}" required
              {% endif %}
            >
          </div>
        </div>
      </div>

