            <circle cx="20.75" cy="20.75" r="8.75" stroke="#393939" stroke-width="1.7" />
            <path d="M32 32L28.25 28.25" stroke="#393939" stroke-width="1.7" stroke-linecap="round" />
          </svg>
        </span>
        {% comment %} Predictive Search starts {% endcomment %}
        <predictive-search>
          <form id='header-predictive-search-form-{{section.id |  handleize}}' action='{{ routes.search_url }}'
            method='get' role='search'>
            <input id='Search' type='search' class='search-input col-12' name='q' value='{{ search.terms | escape }}'
              role='combobox' aria-expanded='false' aria-owns='predictive-search-results'
