      {% comment %} Search Performs Starts {% endcomment %}
      <div class='row p-0 search-results-section' id='emptySearch'>
        <div class='col-12 col-lg-3 p-0'>
          <div id='empltySearchParam' style='display:none;'>
            <p class='search-results-bold-text d-none'>Opps! No Product
            Found.</p>
          </div>
          <ul class='p-0 search-suggestions-container d-flex flex-column' data-suggestion-category='false'>
            <p class='mb-0 search-results-bold-text'>Trending Searches</p>
            {{ section.settings.search_list }}
          </ul>
        </div>
        <div class='col-12 col-lg-9 p-0 '>
          <div class='search-results-tab-container d-flex justify-content-between'>
            <div class='search-results-tabs d-flex'></div>
            <a href='{{ collections[section.settings.home_search_products].url }}' class='shop-all-btn'>Shop All ></a>
          </div>
          <div class='search-results-product-card-container d-flex flex-column flex-lg-row' id='emptySearch'>
            {% for product in collections[section.settings.home_search_products].products
            limit: 4
            %}
            <div class='search-results-product-card d-flex align-items-center flex-lg-column'>
              <div class='search-result-product-image-container'>
                <a href='{{ product.url }}' tabindex='-1'>
                  <img loading='lazy' src='{{ product.featured_image | img_url:'210x280' }}' alt='{{ product.url }}'
                    class='search-result-product-image img-fluid'>
                </a>
              </div>
              <div class='w-100'>
                <p class='mb-0 brand-name'>{{ product.vendor }}</p>
                <p class='mb-0 product-title'>{{ product.title | truncatewords: 8, '...' }}</p>
                <div class="product-price mb-0">
                  <span class="selling-price">{{ product.price | money_without_trailing_zeros }}</span>
                  <span class="old-price">
                    {% if product.compare_at_price > product.price %}
                      {{ product.compare_at_price | money_without_trailing_zeros -}}
                    {% endif %}
                  </span>
                  <span class="discount">
                    {% if product.compare_at_price > product.price %}
                      {{-
                        product.compare_at_price
                        | minus: product.price
                        | times: 100.0
                        | divided_by: product.compare_at_price
                        | money_without_currency
                        | times: 100
                        | remove: '.0'
                      -}}
                      % Off
                    {% endif %}
                  </span>
              </div>
              </div>
            </div>
            {% endfor %}
          </div>
        </div>
      </div>
      {% comment %} If Search Performed {% endcomment %}
      <div id='predictive-search-products' tabindex='-1'></div>
    </div>
  </div>
