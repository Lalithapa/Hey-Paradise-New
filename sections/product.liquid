
{% capture star_ratings %}
  <div class="rating">
    {% for _ in (1..5) %}
      <img src="https://cdn.shopify.com/s/files/1/0673/6396/3172/files/Star_1.svg?v=1692598346" width="14" height="14" alt="stars" class="img-fluid">
    {% endfor %}
  </div>
{% endcapture %}
{% capture product_Custom_Details %}
  <div class="product-desription-details">
                {{ product.description }}
                <!-- Product Sku -->
                <p class="b-pdp_accordion-product_code_wrapper mt-3">
                  Product SKU :
                  <span class="b-pdp_accordion-product_code">
                    {{- product.selected_or_first_available_variant.sku -}}
                  </span>
                </p>
                <!-- Expiry -->
                {% if product.metafields.custom.self_life != blank %}
                  <p class="product-info-details">
                    <span>Shelf Life :</span>
                    {{ product.metafields.custom.self_life | remove: '["' | remove: '"]' }}
                  </p>
                {% endif %}
                <!-- Country Of Origin -->
                {% if product.metafields.custom.country_of_origins != blank %}
                  <p class="product-info-details">
                    <span>Country of Origin :</span>
                    {{ product.metafields.custom.country_of_origins }}
                  </p>
                {% endif %}

                <!-- Manufacturer Name -->
                {% if product.metafields.custom.name_of_mfg != blank %}
                  <p class="product-info-details">
                    <span>Manufacturer Name :</span>
                    {{ product.metafields.custom.name_of_mfg }}
                  </p>
                {% endif %}
                <!-- Address of Manufacturer -->
                {% if product.metafields.custom.address_of_mfg != blank %}
                  <p class="product-info-details">
                    <span>Address of Manufacturer :</span>
                    {{ product.metafields.custom.address_of_mfg }}
                  </p>
                {% endif %}
              </div>
{% endcapture %}
{% liquid
  assign has_size_option = false
  for option in product.options_with_values
    for value in option.values
      if option.name == 'Size'
        assign has_size_option = true
      elsif option.name == 'Color'
        assign has_size_option = false
      endif
    endfor
  endfor
%}

{% assign first_variant = product.selected_or_first_available_variant %}
<section class="bg-white">
  <div class="container">
    <div class="row py-0 py-md-5">
      <div class="col-12 col-md-6">
        <div class="row varient-container sticky200">
          <div class="live-views m-live-views d-md-none">
            <span
              ><img
                loading="lazy"
                src="https://cdn.shopify.com/s/files/1/1743/7443/files/33d72e41b1889e6e1bee4e76cf87b0e9.gif?v=1693810925"
                width="23"
                height="23"
                alt="eyes"
                class="img-fluid"
            ></span>
            <p class="live-viewing-number">{{ 'now' | date: '%N' | modulo: 199 | plus: 59 }} People Are Viewing</p>
          </div>
          <div class="product-preview-container">
            <div class="row">
              {% if product.media.size > 1 %}
                <div
                  class="col-12 col-lg-3 order-2 d-lg-block"
                >
                  <div class="product-preview-track">
                    <button
                      style="transform: rotate(90deg);"
                      class="product-preview-track-prev w-100"
                      direction="previous"
                    >
                      {% render 'carousel-prev-btn-svg' %}
                    </button>

                    <div class="tns-slide position-relative">
                      <div class="product-thumb-slider">
                        {% for media in product.media %}
                          {% if media.media_type == 'image' %}
                            <div class="thumbset-{{ forloop.index }} thumb_container mx-auto">
                              <img
                                {% unless forloop.first %}
                                  loading="lazy"
                                {% endunless %}
                                src="{{ media | image_url }}"
                                class="img-fluid asp34"
                                alt="{{ product.title }}"
                                title="{{ product.title }}"
                              >
                            </div>
                          {% else %}
                            <div class="thumbset-{{ forloop.index }} thumb_container mx-auto">
                              {{
                                media
                                | video_tag:
                                  loop: true,
                                  muted: true,
                                  autoplay: true,
                                  class: 'd-lg-block img-fluid asp34'
                              }}
                            </div>
                          {% endif %}
                        {% endfor %}
                      </div>
                    </div>

                    <button
                      class="product-preview-track-next w-100"
                      style="transform: rotate(90deg);"
                      direction="next"
                    >
                      {% render 'carousel-next-btn-svg' %}
                    </button>
                  </div>
                </div>
              {% endif %}

              <div
                class="col-12{% if product.media.size > 1 %} col-lg-9 {% endif %} order-1 order-lg-3"
              >
                {% capture the_snippet_fave_icon %}{% render 'ssw-widget-faveicon' with product.id %}{% endcapture %}
                {% unless the_snippet_fave_icon contains 'Liquid error' %}
                  {{ the_snippet_fave_icon }}
                {% endunless %}
                <div class="product-main-slider row d-flex flex-nowrap overflow-hidden">
                  {% for media in product.media %}
                    <div class="childset-{{ forloop.index }} col-12">
                      {% if media.media_type == 'image' %}
                        <a
                          href="{{ media | image_url }}"
                          data-fancybox="slideShow"
                          data-caption="{{ product.variant.title }}"
                        >
                          <img
                            src="{{ media | image_url }}"
                            class="img-fluid asp34"
                            alt="{{ product.title |  append: forloop.index }}"
                            title="{{ product.title }}"
                          >
                        </a>
                      {% else %}
                        <a
                          href="{{ media.sources[0].url }}"
                          data-fancybox="slideShow"
                          data-caption="{{ product.variant.title }}"
                        >
                          {{
                            media
                            | video_tag: loop: true, muted: true, autoplay: true, class: 'd-block img-fluid asp34'
                          }}
                        </a>
                      {% endif %}
                    </div>
                  {% endfor %}
                </div>
                {% if product.title contains 'E-Gift card' %}
                  <div id="data-e-gift">
                    <span class="e-email"></span>
                    <span class="e-name"></span>
                    <span class="e-message"></span>
                    <span class="e-date"></span>
                  </div>
                {% endif %}
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="col-12 col-sm-6">
        <div class="product-details-container">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when '@app' %}
                {% render block %}
              {% when 'social-share' %}
                  <div class="social-share-section pt-3 pb-2 w-100 h-100">
                    {% render 'social-share' %}
                  </div>
              {% when 'title' %}
                <div class="product-info">
                  <p class="product-brand">{{ product.vendor }}</p>
                  <p class="product-title" productPage>{{ product.title }}</p>
                </div>
              {% when 'pricing' %}
                <div class="product-pricing">
                  <div class="product-prices">
                    <span>
                      <span class="selling-price">{{ first_variant.price | money_without_trailing_zeros }}</span>
                      <span class="old-price">
                        {% if first_variant.compare_at_price > first_variant.price %}
                          {{ first_variant.compare_at_price | money_without_trailing_zeros -}}
                        {% endif %}
                      </span>
                      <span class="discount">
                        {% if first_variant.compare_at_price > first_variant.price %}
                          {{-
                            first_variant.compare_at_price
                            | minus: first_variant.price
                            | times: 100.0
                            | divided_by: first_variant.compare_at_price
                            | money_without_currency
                            | times: 100
                            | remove: '.0'
                          -}}
                          % Off
                        {% endif %}
                      </span>
                    </span>
                  </div>
                  <p class="tax-info">Inclusive Of All Taxes</p>
                </div>
              {% when 'variant_picker' %}
                <div class="product-varient-selector">
                  {% if product.available and product.variants.size > 1 %}
                    <div>
                      <p class="current-shade">
                        {% if product.title == 'E-Gift card' -%}
                          Vibe
                        {%- elsif has_size_option -%}
                          SIZE
                        {%- else -%}
                          SHADE
                        {%- endif -%}
                        : <span>{{ first_variant.title }}</span>
                      </p>
                    </div>
                  {% endif %}
                  <div class="swatches shades">
                    {% if product.available and product.variants.size > 1 %}
                      {% render 'swatches'
                        with 'Color' as swatch,
                        class_variable: class_variable,
                        sectionId: section.id,
                        product: product,
                        firstAvailable: first_variant,
                        showall: true,
                        showdropdown: true
                      %}
                    {% endif %}
                  </div>
                </div>
