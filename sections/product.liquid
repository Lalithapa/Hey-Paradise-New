<script>
  $(document).ready(function () {
    $('#offerToCopy').click(function (event) {
      event.preventDefault();
      let couponCode = $(this).attr('coupon');
      $.ajax({
        url: `{{ shop.url }}/discount/${couponCode}`,
        success: function (data) {
          alert('Your discount has been applied!');
          $(this).text('Coupon Applied');
        },
      });
    });
    $('.view-gifts-slide-btn').click(function () {
      $('.view-gifts-slide').addClass('active');
      $('.cart-overlay').addClass('cart-overlay-open');
    });
    $('.view-gifts-slide-close-btn, .cart-overlay').click(function () {
      $('.view-gifts-slide').removeClass('active');
      $('.cart-overlay').removeClass('cart-overlay-open');
    });
  });
</script>

<script>
    let first_Selected = {{ first_variant.featured_media.position }}; 
    let vertical_slider = tns({
      container: '.product-thumb-slider',
      items: 4,
      gutter: 12,
      axis: 'vertical',
      mouseDrag: true,
      controls: true,
      loop: false,
      nav:false,
      arrowKeys: true,
      navAsThumbnails: false,
      prevButton: '.product-preview-track-prev',
      nextButton: '.product-preview-track-next'
      });
    
      let mainSlider = tns({
        container: '.product-main-slider',
        items: 1,
        mouseDrag: true,
        navContainer: '.product-thumb-slider',
        controlsContainer:'.product-thumb-slider',
        navAsThumbnails: true,
        mode: 'gallery',
        controls: false,
        loop: false
      });

      if (mainSlider && vertical_slider) {
        mainSlider.goTo(first_Selected -1);
        vertical_slider.goTo(first_Selected -1);
        $(`.product-preview-container`).fadeIn(100);
      }
    document.addEventListener("DOMContentLoaded", (event) => {
      setInterval(liveViewsCounter, 120000);
    });
    const liveViewsCounter = () => {
      let CurruntViews = Math.floor(Math.random() * (199 - 59 + 1)) + 59;
      return $('.live-viewing-number').text(`${CurruntViews} People Are Viewing`);
    }

    liveViewsCounter();

    {% if product.title contains "E-Gift card" %}
    let giftHandler = $('#data-e-gift');
    
    $('.giftHandle').on('input',function() {
      var inputValue = $(this).val();  // Fix: Use $(param).val() to get the input value
      var fillerElem = $(this).attr("filler");
      // Update the content of the output div
      if (fillerElem == 'email') {
        $(giftHandler).find('.e-email').text(inputValue);
      } else if (fillerElem == 'name') {
        $(giftHandler).find('.e-name').text(inputValue);
      } else if (fillerElem == 'message') {
        $(giftHandler).find('.e-message').text(inputValue);
      } else if (fillerElem == 'date') {
        $(giftHandler).find('.e-date').text(inputValue);
      }
    });
    {% endif %}
</script>


